<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" >
    <children>
        <Label layoutX="76.0" layoutY="66.0" text="Add Part">
            <font>
                <Font name="Arial Bold" size="18.0" />
            </font>
        </Label>
        <RadioButton fx:id="inHouseRadioButton" layoutX="188.0" layoutY="67.0" mnemonicParsing="false" onAction="#inHouseRadioButtonSelected" selected="true" text="In-House">
            <font>
                <Font size="14.0" />
            </font>
        </RadioButton>
        <RadioButton fx:id="outsourcedRadioButton" layoutX="379.0" layoutY="67.0" mnemonicParsing="false" onAction="#outsourcedRadioButtonSelected" text="Outsourced">
            <font>
                <Font size="14.0" />
            </font>
        </RadioButton>
        <VBox layoutX="76.0" layoutY="129.0" prefHeight="250.0" prefWidth="100.0" spacing="15.0">
            <children>
                <AnchorPane prefHeight="275.0" prefWidth="100.0">
                    <children>
                        <Label layoutX="1.0" layoutY="5.0" prefHeight="17.0" prefWidth="17.0" text="ID">
                            <font>
                                <Font name="Arial" size="14.0" />
                            </font>
                        </Label>
                        <Label layoutX="1.0" layoutY="40.0" text="Name">
                            <font>
                                <Font name="Arial" size="14.0" />
                            </font>
                        </Label>
                        <Label layoutX="1.0" layoutY="75.0" text="Inv">
                            <font>
                                <Font name="Arial" size="14.0" />
                            </font>
                        </Label>
                        <Label layoutX="1.0" layoutY="110.0" text="Price/Cost">
                            <font>
                                <Font name="Arial" size="14.0" />
                            </font>
                        </Label>
                        <Label layoutX="1.0" layoutY="145.0" text="Max">
                            <font>
                                <Font name="Arial" size="14.0" />
                            </font>
                        </Label>
                        <Label fx:id="partSwappableLabel" layoutX="1.0" layoutY="180.0" text="Machine ID">
                            <font>
                                <Font name="Arial" size="14.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
            </children>
        </VBox>
        <VBox layoutX="346.0" layoutY="129.0" prefHeight="250.0" prefWidth="100.0">
            <children>
                <AnchorPane prefHeight="249.0" prefWidth="100.0">
                    <children>
                        <Label alignment="CENTER" layoutX="30.0" layoutY="145.0" text="Min">
                            <font>
                                <Font name="Arial" size="14.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
            </children>
        </VBox>
        <TextField fx:id="partIdTextField" disable="true" layoutX="180.0" layoutY="129.0" promptText=" Auto Gen - Disabled">
            <font>
                <Font name="Arial" size="13.0" />
            </font></TextField>
        <TextField fx:id="partNameTextField" layoutX="180.0" layoutY="165.0">
            <font>
                <Font name="Arial" size="13.0" />
            </font></TextField>
        <TextField fx:id="partInvTextField" layoutX="180.0" layoutY="198.0">
            <font>
                <Font name="Arial" size="13.0" />
            </font></TextField>
        <TextField fx:id="partPriceTextField" layoutX="180.0" layoutY="233.0">
            <font>
                <Font name="Arial" size="13.0" />
            </font></TextField>
        <TextField fx:id="partMaxTextField" layoutX="180.0" layoutY="268.0">
            <font>
                <Font name="Arial" size="13.0" />
            </font></TextField>
        <TextField fx:id="partSwappableTextField" layoutX="180.0" layoutY="307.0">
            <font>
                <Font name="Arial" size="13.0" />
            </font></TextField>
        <TextField fx:id="partMinTextField" layoutX="417.0" layoutY="268.0">
            <font>
                <Font name="Arial" size="13.0" />
            </font></TextField>
        <Button fx:id="partSaveButton" layoutX="319.0" layoutY="411.0" mnemonicParsing="false" onAction="#save" text="Save" />
        <Button fx:id="partCancelButton" layoutX="379.0" layoutY="411.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
    </children>
</AnchorPane>
